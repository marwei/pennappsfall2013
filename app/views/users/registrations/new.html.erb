<%= form_for(resource, :as => resource_name, :html => {class:"form-signin"}, :url => registration_path(resource_name)) do |f| %>
  <h2 class:"form-signin-heading">Thank you for signing up!</h2>
  <%= f.email_field :email, :autofocus => true, class:"form-control", placeholder:"Email address" %>
  <%= f.password_field :password, class:"form-control", placeholder:"Password"%>
  <%= f.password_field :password_confirmation, class:"form-control", placeholder:"Password Confirmation"%>
  <%= f.submit "Sign up", class:"btn btn-lg btn-primary btn-block" %>
  <!--<%= render "devise/shared/links" %>-->
<% end %>
